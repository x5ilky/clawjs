import "intrin"

impl Runtime for number {
    fn sizeof() number {
        return 1
    }
    
    fn to_scratch_value(self: Self) JsObject {
        obj := $.create_js_object()
        // | { key: "Float", value: number }
        obj:set("key", "Float")
        obj:set("value", self)

        return obj
    }
}

